                                            <label style="color: teal;font-weight: 500; margin-left:80px;">Country:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorCountry"></label>
                                            <label style="color: teal;font-weight: 500; margin-left:60px;">State:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorState"></label>
                                            <label style="color: teal;font-weight: 500; margin-left:60px;">City:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorCity"></label>
                                        </div>
                                        <div class="form-group">
                                            <label style="color: teal;font-weight: 500; margin-left:80px;">PinCode:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorPinCode"></label>
                                        </div>
                                        <div class="form-group">
                                            <label style="color: teal;font-weight: 500; margin-left:80px;">Address:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorAddress"></label>
                                        </div>
                                    </div>
