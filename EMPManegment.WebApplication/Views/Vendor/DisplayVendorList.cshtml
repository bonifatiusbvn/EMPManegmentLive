                            <thead>
                                <tr>
                                    <th>CompanyName</th>
                                    <th>VendorName</th>
                                    <th>VendorEmail</th>
                                    <th>VendorMobileNo</th>
                                    <th>CompanyContac</th>
                                    <th>CompanyEmail</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                    <div class="modal fade bs-example-modal-lg" id="VendorDetailsModel" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <!-- Company Details -->
                                        <div class="box-1">
                                            <div>
                                                <h5 class="text-center" style="padding-bottom:10px; color: black;">Company Details</h5>
                                            </div>
                                            <div class="form-group control-label">
                                                <label style="color: teal;font-weight: 500;">Name:</label>
                                                <label style="color: black;font-weight: 500; margin-left: 68px" id="VendorCompany"></label>
                                            </div>
                                            <div class="form-group">
                                                <label style="color: teal;font-weight: 500;">CompanyType:</label>
                                                <label style="color: black;font-weight: 500; margin-left: 12px" id="VendorCompanyType"></label>
                                            </div>
                                            <div class="form-group">
                                                <label style="color: teal;font-weight: 500;">Email:</label>
                                                <label style="color: black;font-weight: 500; margin-left: 70px" id="VendorCompanyEmail"></label>
                                            </div>
                                            <div class="form-group">
                                                <label style="color: teal;font-weight: 500;">ContactNumber:</label>
                                                <label style="color: black;font-weight: 500;" id="VendorCompanyNumber"></label>
                                            </div>
                                        </div>
                                        <!-- Vendor Details -->
                                        <div class="box-2">
                                            <div>
                                                <h5 class="text-center" style="padding-bottom:10px; color: black;">Vendor Details</h5>
                                            </div>
                                            <div class="form-group">
                                                <label style="color: teal;font-weight: 500;">Name:</label>
                                                <label style="color: black;font-weight: 500; margin-left: 31px" id="VendorFirstName"></label>
                                            </div>
