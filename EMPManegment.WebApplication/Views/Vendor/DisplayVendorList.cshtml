                                            <label style="color: teal;font-weight: 500; margin-left:30px;">AccountHolderName:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorAccountHolderName"></label>
                                            <label style="color: teal;font-weight: 500; margin-left:80px;">Gstnumber:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorGstnumber"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button class="btn btn-danger btn-default m-1" data-bs-dismiss="modal"><i class="ri-close-line me-1 align-middle"></i> Close</button>
                                </div>
                            </div><!-- /.modal-content -->
                        </div><!-- /.modal-dialog -->
                    </div><!-- /.modal -->
                    <!-- Add Vendor Modal -->
