                                        <div>
                                            <h5 class="text-center" style="padding-bottom:10px; color: black;">Bank Details</h5>
                                        </div>
                                        <div class="form-group">
                                            <label style="color: teal;font-weight: 500; margin-left:30px;">BankName:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorBankName"></label>
                                            <label style="color: teal;font-weight: 500; margin-left:195px;">AccountNo:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorBankAccountNo"></label>
                                        </div>
                                        <div class="form-group">
                                            <label style="color: teal;font-weight: 500; margin-left:30px;">BankBranch:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorBankBranch"></label>
                                            <label style="color: teal;font-weight: 500; margin-left:185px;">Ifsc Code:</label>
                                            <label style="color: black;font-weight: 500;" id="VendorBankIfsc"></label>
                                        </div>
