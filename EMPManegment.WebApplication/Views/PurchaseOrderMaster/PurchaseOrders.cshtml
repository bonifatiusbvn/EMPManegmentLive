@*@using X.PagedList;
@using X.PagedList.Mvc.Core;*@
@{
    ViewData["Title"] = "CreateOrder";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}


<div class="row" style="margin-top:80px;">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0">Orders</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Ecommerce</a></li>
                    <li class="breadcrumb-item active">Orders</li>
                </ol>
            </div>

        </div>
    </div>
</div>
<!-- end page title -->
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <div class="card" id="orderList">
                <div class="card-header border-0">
                    <div class="row align-items-center gy-3">
                        <div class="col-sm">
                            <h5 class="card-title mb-0">Order History</h5>
                            <input hidden id="txtPOId" type="text" value="@UserSession.ProjectId" />
                        </div>
                        @if (UserSession.FormPermisionData.Any(a => a.FormName == "Create Purchase Order" && (a.Add == true)))
                        {
                            <div class="col-sm-auto">
                                <div class="d-flex gap-1 flex-wrap">
                                    <button class="btn btn-info add-btn btn-md" id="create-btn"onclick="createPO()"><i class="ri-add-line align-bottom me-1"></i>Purchase Order</button>
                                    <button class="btn btn-soft-danger" id="remove-actions" onClick="deleteMultiple()"><i class="ri-delete-bin-2-line"></i></button>
                                </div>
                            </div>
                        }
                    </div>
                </div>
                <div class="card-body border border-dashed border-end-0 border-start-0">
                </div>
                <div class="card-body pt-0">
                    <div>
                        <ul class="nav nav-tabs nav-tabs-custom nav-primary mb-3" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active fw-semibold" id="allordersactive" data-bs-toggle="tab" href="#project-overview" role="tab">
                                    <i class="ri-store-2-fill me-1 align-bottom"></i> All Orders <span class="badge bg-secondary align-middle ms-1">@ViewBag.ordersList</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link fw-semibold" id="deliveredactive" data-bs-toggle="tab" href="#status-delivered" role="tab">
                                    <i class="ri-checkbox-circle-line me-1 align-bottom"></i> Delivered <span class="badge bg-secondary align-middle ms-1">@Model.Count(t => t.DeliveryStatus == "Delivered")</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link fw-semibold" id="pickupactive" data-bs-toggle="tab" href="#status-pickups" role="tab">
                                    <i class="ri-truck-line me-1 align-bottom"></i> Pickups <span class="badge bg-secondary align-middle ms-1">@Model.Count(t => t.DeliveryStatus == "Pickups")</span>
                                </a>
                            </li>
