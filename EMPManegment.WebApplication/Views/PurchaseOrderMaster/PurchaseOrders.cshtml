                                    <svg xmlns="http://www.w3.org/2000/svg" class="me-1 align-bottom" width="17" height="17" viewBox="0 0 24 24"><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 20.777a8.942 8.942 0 0 1-2.48-.969M14 3.223a9.003 9.003 0 0 1 0 17.554m-9.421-3.684a8.961 8.961 0 0 1-1.227-2.592M3.124 10.5c.16-.95.468-1.85.9-2.675l.169-.305m2.714-2.941A8.954 8.954 0 0 1 10 3.223" /></svg> Inprogress <span class="badge bg-secondary align-middle ms-1">@Model.Count(t => t.DeliveryStatus == "Inprogress")</span>
                                </a>
                            </li>
                        </ul>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="tab-content text-muted">
                                    <div class="tab-pane fade show active" id="project-overview" role="tabpanel">
                                        <div class="tab-pane" id="status-all" role="tabpanel">
                                            <div class="table-responsive table-card mb-1">
                                                <table class="table table-bordered dt-responsive nowrap table-striped align-middle dataTable no-footer dtr-inline" id="orderTable">
                                                    <thead class="text-muted table-info">
                                                        <tr class="text-uppercase">

                                                            <th>Order ID</th>
                                                            <th>Company</th>
                                                            <th>Order Date</th>
                                                            <th>Amount</th>
                                                            <th>Payment Method</th>
                                                            <th>Delivery Status</th>
                                                            @if (UserSession.FormPermisionData.Any(a => a.FormName == "Create Purchase Order" && (a.Edit == true || a.Delete == true)))
                                                            {
                                                                <th>Action</th>
                                                            }
                                                        </tr>
                                                    </thead>
                                                    <tbody class="list form-check-all">
                                                        @foreach (var item in Model)
                                                        {
                                                            <tr>

                                                                <td class="id"><a href="/PurchaseOrderMaster/PurchaseOrderDetails/?OrderId=@item.OrderId" class="fw-medium link-primary">@item.OrderId</a></td>
                                                                <td class="company_name">@item.CompanyName</td>
                                                                <td class="date">@CommonHelper.GetCommonDateFormat(item.OrderDate)</td>
                                                                <td class="total_amount">@item.TotalAmount</td>
                                                                <td class="payment">@item.PaymentMethodName</td>
                                                                <td class="status" id="DeliveryStatus">
                                                                    <span class="badge bg-warning-subtle text-warning text-uppercase">@item.DeliveryStatus</span>
                                                                </td>
                                                                @if (UserSession.FormPermisionData.Any(a => a.FormName == "Create Purchase Order" && (a.Edit == true || a.Delete == true)))
                                                                {
                                                                    <td>
                                                                        <ul class="list-inline hstack gap-2 mb-0">
                                                                            @if (UserSession.FormPermisionData.Any(a => a.FormName == "Create Purchase Order" && (a.Edit == true)))
                                                                            {
                                                                                <li class="list-inline-item edit" data-bs-toggle="tooltip" data-bs-trigger="hover" data-bs-placement="top" title="Edit">
                                                                                    <a href="/PurchaseOrderMaster/CreatePurchaseOrder?id=@item.Id" class="btn text-primary d-inline-block edit-item-btn">
                                                                                        <i class="fa-regular fa-pen-to-square"></i>
                                                                                    </a>
                                                                                </li>
                                                                            }
